    "mini-css-extract-plugin": "^1.6.2",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.1",
    "path-parse": "^1.0.7",
    "lodash": ">=4.17.21",
    "jquery": ">=3.6.0",
    "kind-of": ">=6.0.3"
