    "pretty-bytes": "^5.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-flow": "^7.16.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.1",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.2.7",
